####### Compile

main.o: main.cpp GameManger.hpp \
		Frontground.hpp \
		Layer.hpp \
		Penguin.hpp \
		Constants.hpp \
		ParticleSystem.hpp \
		ObstacleManager.hpp \
		Obstacle.hpp \
		Background.hpp \
		UI.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

GameManger.o: GameManger.cpp GameManger.hpp \
		Frontground.hpp \
		Layer.hpp \
		Penguin.hpp \
		Constants.hpp \
		ParticleSystem.hpp \
		ObstacleManager.hpp \
		Obstacle.hpp \
		Background.hpp \
		UI.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o GameManger.o GameManger.cpp

Penguin.o: Penguin.cpp Penguin.hpp \
		Constants.hpp \
		ParticleSystem.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Penguin.o Penguin.cpp

Background.o: Background.cpp Background.hpp \
		Constants.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Background.o Background.cpp

Obstacle.o: Obstacle.cpp Obstacle.hpp \
		Constants.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Obstacle.o Obstacle.cpp

ObstacleManager.o: ObstacleManager.cpp ObstacleManager.hpp \
		Obstacle.hpp \
		Constants.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ObstacleManager.o ObstacleManager.cpp

Frontground.o: Frontground.cpp Frontground.hpp \
		Layer.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Frontground.o Frontground.cpp

Layer.o: Layer.cpp Layer.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Layer.o Layer.cpp

UI.o: UI.cpp UI.hpp \
		GameManger.hpp \
		Frontground.hpp \
		Layer.hpp \
		Penguin.hpp \
		Constants.hpp \
		ParticleSystem.hpp \
		ObstacleManager.hpp \
		Obstacle.hpp \
		Background.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o UI.o UI.cpp

